    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "hardhat": "^2.11.2",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.7.0",
    "@weichain/safe-core-sdk-types": "^1.6.1",
    "@weichain/safe-core-sdk-utils": "^1.4.1",
    "web3": "^1.8.0",
    "web3-core": "^1.8.0",
    "web3-utils": "^1.8.0"
