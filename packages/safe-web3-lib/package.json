    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hardhat": "^2.9.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typechain": "^7.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@gnosis.pm/safe-core-sdk-types": "^1.2.0",
    "@gnosis.pm/safe-core-sdk-utils": "^1.2.0"
  },
  "peerDependencies": {
    "web3": "^1.7.0"
